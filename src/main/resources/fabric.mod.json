{
	"schemaVersion": 1,
	"id": "finerf5",
	"version": "${version}",
	"name": "Finer F5",
	"description": "A simple fabric mod that provides more refined control to your perspective switching",
	"authors": [
		"Hol1ow"
	],
	"contact": {
		"homepage": "https://github.com/Hol1ow/FinerF5",
		"sources": "https://github.com/Hol1ow/FinerF5",
		"issues": "https://github.com/Hol1ow/FinerF5/issues"
	},
	"license": "GPL-3.0 license",
	"icon": "assets/finerf5/icon.png",
	"environment": "*",
	"accessWidener" : "finerf5.accesswidener",
	"entrypoints": {
		"main": [
			"com.pang.finerf5.FinerF5"
		],
		"client": [
			"com.pang.finerf5.FinerF5Client"
		]
	},
	"mixins": [
		"finerf5.mixins.json",
		{
			"config": "finerf5.client.mixins.json",
			"environment": "client"
		}
	],
	"depends": {
		"fabricloader": ">=0.15.11",
		"minecraft": "~1.20.6",
		"java": ">=21",
		"fabric-api": "*"
	},
	"suggests": {
		"another-mod": "*"
	}
}